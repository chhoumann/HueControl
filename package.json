{
  "name": "hue-control",
  "version": "1.0.0",
  "description": "NuxtJS HueControl",
  "author": "Christian Bager Bach Houmann",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development HOST=0.0.0.0 PORT=3000 nodemon server/index.js --watch server",
