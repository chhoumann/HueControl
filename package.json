    "postcss-preset-env": "6.7.0",
    "vue-slider-component": "3.1.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.13.0",
    "@nuxtjs/vuetify": "1.11.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "vue-jest": "^4.0.0-0"
  }
